 class Item {

            String ItemId;
            String Name;
            String Description;
            double Price;
            int Quantity;

            Item(Item item,int quantity)
            {
                this.ItemId=item.ItemId;
                this.Name =item.Name;
                this.Description=item.Description
                this.Price= item.Price;
                this.Quantity=quantity;
            }
    }

    